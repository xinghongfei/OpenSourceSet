<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    package="com.example.xinghongfei.opensourceset">

    <application
        tools:replace="android:label"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        >
        <activity android:name=".MainActivity"
            android:theme="@style/First">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".view.button.ButtonActivity"
            android:theme="@style/First"
            />

        <activity android:name=".main.DetailActivity"
            android:theme="@style/First"
            />

        <activity android:name=".view.refresh.RegreshActivity"
            android:theme="@style/First"
            />

        <activity android:name=".view.indicator.IndicatorActivity"
            android:theme="@style/AppTheme.Indicator"/>



    </application>

</manifest>